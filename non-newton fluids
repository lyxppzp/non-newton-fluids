m = float(input("введите массу пули, гр: ")) / 1000
v = float(input("введите скорость пули, м/с: "))
k = float(input("введите калибр пули, мм: ")) / 10
r = k/2
s = 3.14*r*r
Ek = m*v*v/2
Eu = Ek/s
print("[Ek]Кинетическая Энергия, Дж: "+str(Ek))
print("[Eu]Удельная Кинетическая Энергия, Дж/см2: "+str(Eu))
